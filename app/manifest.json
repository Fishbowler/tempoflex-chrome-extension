{
  "name": "__MSG_appName__",
  "version": "0.7.4",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "man-flexing-legs_16.png",
    "32": "man-flexing-legs_32.png",
    "48": "man-flexing-legs_48.png",
    "128": "man-flexing-legs_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "optional_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "16": "man-flexing-legs_16.png",
      "32": "man-flexing-legs_32.png",
      "48": "man-flexing-legs_48.png",
      "128": "man-flexing-legs_128.png"
    },
    "default_title": "TempoFlex",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
