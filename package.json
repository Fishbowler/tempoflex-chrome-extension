{
  "name": "tempoflex",
  "private": true,
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "build": "gulp",
    "package": "gulp package",
    "test": "jest",
    "test:ci": "jest && cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@types/chrome": "0.0.88",
    "acorn": "^6.2.1",
    "browserify": "^16.4.0",
    "coveralls": "^3.0.5",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-zip": "^5.0.0",
    "humanize-duration": "^3.20.1",
    "jest": "^24.8.0",
    "merge-stream": "^2.0.0",
    "nock": "^10.0.6",
    "rewire": "^4.0.1",
    "sinon-chrome": "^3.0.1",
    "timekeeper": "^2.2.0",
    "vinyl-source-stream": "^2.0.0",
    "workingday-uk": "^1.1.2",
    "xmlhttprequest": "^1.8.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/scripts/**/*.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {}
}
